	<h2 id="my-events"> My Events: </h2>

<div class="my-events">
	 <% @events.each do |event| %>
	 <div style="border-width: 2px; border-radius: 5px; border-style: solid; border-color: #286090; margin-top: 10px; margin-left:300px; margin-right:300px; background-color: #C0C0C0">
		Event: <%= event.name %> 
<br>
	<p>Description: <%= event.description %></p>
	<p>Date: <%= event.time %></p>
	<p>Location: <%= event.location %></p>
</div>
	<% end %>
</div>
